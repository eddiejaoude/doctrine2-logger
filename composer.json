{
    "name" : "eddiejaoude/doctrine2-logger",
    "description" : "Zend Framework 2 & Doctrine 2 SQL Logger using Zend Log",
    "type" : "library",
    "authors" : [{
        "name" : "Eddie Jaoude",
        "email" : "eddie@jaoudestudios.com"
    }
    ],
    "license": "MIT",
    "keywords" : [
        "zend",
        "zend framework 2",
        "zf2",
        "logger",
        "log",
        "doctrine 2",
        "doctrine",
        "sql log"
    ],
    "require" : {
        "php" : ">=5.5",
        "zendframework/zend-log" : "2.2.*",
        "doctrine/orm": ">2.4",
        "eddiejaoude/zf2-logger" : "0.*",
        "zendframework/zend-di" : "2.2.*",
        "zendframework/zend-eventmanager" : "2.2.*",
        "zendframework/zend-mvc" : "2.2.*"
    },
    "require-dev" : {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery" : "0.8.0",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zendframework": "2.2.*"
    },
    "autoload" : {
        "psr-4" : {
            "EddieJaoude\\Doctrine2Logger\\" : "src/Doctrine2Logger"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
